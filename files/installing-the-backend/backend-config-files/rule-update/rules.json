{
  "catalogs": [
    {
      "user": "admin",
      "catalog": ".*",
      "allow": "all"
    },
    {
      "catalog": "postgresql",
      "allow": "read-only"
    },
    {
      "catalog": "system",
      "allow": "none"
    }
  ],
  "schemas": [
    {
      "user": ".*",
      "schema": ".*",
      "owner": false
    }
  ],
  "tables": [
    {
      "user": ".*",
      "privileges": []
    }
  ]
}